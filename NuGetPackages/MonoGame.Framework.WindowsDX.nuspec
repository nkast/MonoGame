<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
    <id>MonoGame.Framework.WindowsDX.9000</id>
    <version>0.0.0.0</version>
    <title>MonoGame.Framework.WindowsDX.9000</title>
    <authors>Kastellanos Nikos, MonoGame Team</authors>
    <owners>Kastellanos Nikos</owners>
    <licenseUrl>https://github.com/nkast/MonoGame/blob/develop/LICENSE.txt</licenseUrl>
    <projectUrl>https://github.com/nkast/MonoGame</projectUrl>
    <!--<iconUrl>https://pbs.twimg.com/profile_images/487954549441691649/O3KsHAsb_400x400.png</iconUrl>-->
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      This is a custom build of MonoGame framework. Includes bugfixes, performance improvements, less memory garbage per frame, and better memory management.

      This package provides you with MonoGame Framework that uses DirectX for rendering and works on Windows.</description>
    <summary>This package provides you with MonoGame Framework that uses DirectX for rendering and works on Windows.</summary>
    <copyright>Copyright © 2009-2020 The MonoGame Team, Copyright © 20013-2020 Kastellanos Nikos</copyright>
    <language>en-US</language>
    <dependencies>
        <group targetFramework=".NETFramework4.0">
            <dependency id="SharpDX" version="4.0.1" />
            <dependency id="SharpDX.Direct2D1" version="4.0.1" />
            <dependency id="SharpDX.Direct3D11" version="4.0.1" />
            <dependency id="SharpDX.DXGI" version="4.0.1" />
            <dependency id="SharpDX.XAudio2" version="4.0.1" />
            <dependency id="SharpDX.MediaFoundation" version="4.0.1" />
            <dependency id="SharpDX.XInput" version="4.0.1" />
            <dependency id="SharpDX.Direct3D9" version="4.0.1" />
        </group>        
        <group targetFramework="net5.0-windows7.0">
          <dependency id="SharpDX" version="4.0.1" exclude="Build,Analyzers" />
          <dependency id="SharpDX.DXGI" version="4.0.1" exclude="Build,Analyzers" />
          <dependency id="SharpDX.Direct2D1" version="4.0.1" exclude="Build,Analyzers" />
          <dependency id="SharpDX.Direct3D11" version="4.0.1" exclude="Build,Analyzers" />
          <dependency id="SharpDX.MediaFoundation" version="4.0.1" exclude="Build,Analyzers" />
          <dependency id="SharpDX.XAudio2" version="4.0.1" exclude="Build,Analyzers" />
          <dependency id="SharpDX.XInput" version="4.0.1" exclude="Build,Analyzers" />
        </group>
    </dependencies>
  </metadata>
  <files>
    <file src="NuGetPackages\build\WindowsDX\MonoGame.Framework.WindowsDX.9000.targets" target="build\MonoGame.Framework.WindowsDX.9000.targets" />
    
    <!-- C# Assemblies -->
    <file src="MonoGame.Framework\bin\Windows\AnyCPU\Release\MonoGame.Framework.dll" target="lib\net40\MonoGame.Framework.dll" />
    <file src="MonoGame.Framework\bin\Windows\AnyCPU\Release\MonoGame.Framework.xml" target="lib\net40\MonoGame.Framework.xml" />
    
    <file src="Artifacts\MonoGame.Framework\WindowsDX\Release\net5.0-windows\MonoGame.Framework.dll" target="lib\net5.0-windows7.0\MonoGame.Framework.dll" />
    <file src="Artifacts\MonoGame.Framework\WindowsDX\Release\net5.0-windows\MonoGame.Framework.xml" target="lib\net5.0-windows7.0\MonoGame.Framework.xml" />
  </files>
</package>
